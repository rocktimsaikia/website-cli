{
	"name": "rocktim",
	"version": "1.0.1",
	"description": "CLI version of my website",
	"keywords": ["rocktimsaikia", "rocktim", "cli"],
	"repository": "rocktimsaikia/website-cli",
	"funding": "https://github.com/sponsors/rocktimsaikia",
	"license": "MIT",
	"author": {
		"name": "Rocktim Saikia",
		"email": "rocktimthedev@gmail.com",
		"url": "https://rocktimsaikia.dev"
	},
	"type": "module",
	"bin": "./dist/cli.js",
	"files": ["dist"],
	"scripts": {
		"build": "tsup",
		"clean": "rm -rf ~/.notes/*",
		"demo": "tsx src/cli.ts",
		"format": "biome format --write .",
		"lint": "biome lint .",
		"lint:fix": "biome lint --write --unsafe .",
		"test": "echo \"Error: no test specified\" && exit 0",
		"release": "np --no-publish"
	},
	"dependencies": {
		"chalk": "^5.4.1",
		"cli-select": "^1.1.2",
		"figures": "^6.1.0",
		"open": "^10.1.1"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@types/node": "^20.17.12",
		"np": "^10.2.0",
		"tsup": "^8.3.5",
		"tsx": "^4.19.2",
		"typescript": "^5.7.3"
	},
	"packageManager": "pnpm@10.8.1",
	"engines": {
		"node": ">=18"
	},
	"tsup": {
		"entry": ["src/cli.ts"],
		"minify": true,
		"clean": true,
		"format": "esm"
	}
}
